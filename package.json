{
  "name": "@mindstudio-ai/local-model-tunnel",
  "version": "0.3.3",
  "description": "Run local AI models with MindStudio",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mindstudio-local": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup && node dist/cli.js",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build",
    "local-update": "npm run build && npm link",
    "clean:destructive": "bash scripts/clean-destructive.sh"
  },
  "keywords": [
    "mindstudio",
    "ollama",
    "local-models",
    "ai"
  ],
  "author": "MindStudio",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mindstudio-ai/mindstudio-local-model-tunnel"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "ollama": "^0.5.0",
    "open": "^10.2.0",
    "ora": "^8.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.10",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
